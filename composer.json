{
    "name": "czim/laravel-paperclip",
    "description": "Laravel Eloquent file attachment solution",
    "keywords": [ "file", "attachment", "laravel", "upload" ],
    "homepage": "https://github.com/czim",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/daniel-de-wit/file-handling"
        }
    ],
    "require": {
        "php": "^8.1",
        "czim/file-handling": "dev-master",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "nunomaduro/larastan": "^2.5",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Paperclip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Paperclip\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\Paperclip\\Providers\\PaperclipServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
